{
  "name": "langchain-ts-starter",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --declaration --outDir dist/",
    "start": "concurrently \"yarn start-server\"",
    "start-ui": "cd ui && yarn build",
    "start-server": "tsx -r dotenv/config src/index.ts",
    "start:dist": "yarn build && node -r dotenv/config dist/index.js",
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\""
  },
  "keywords": [
    "langchain",
    "starter",
    "template",
    "node",
    "typescript",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.6.6",
    "@types/socket.io": "^3.0.2",
    "@types/turndown": "^5.0.1",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crawlee": "^3.3.1",
    "create-react-app": "^5.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.7.0",
    "hnswlib-node": "^1.4.2",
    "langchain": "^0.0.39",
    "mongodb": "5.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.3",
    "node-html-markdown": "^1.3.0",
    "node-spider": "^1.4.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "pdf-to-text": "^0.0.7",
    "pg": "8.7.1",
    "playwright": "^1.32.3",
    "socket-io": "^1.0.0",
    "telegraf": "^4.12.2",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/js-yaml": "^4",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  }
}
